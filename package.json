{
  "name": "secret-sentinel-scanner-rotator",
  "version": "0.2.0",
  "description": "A small TypeScript CLI that scans files for secret-like patterns and supports pluggable rotators (dry-run/apply).",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
  "test": "vitest run",
  "lint": "eslint src test --ext .ts",
    "format": "prettier --write .",
    "clean": "rimraf dist .sentinel_tmp tmp-demo tmp-try tmp-config.txt"
  },
  "keywords": [
    "secret",
    "scanner",
    "rotator",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
  "@types/jszip": "^3.4.1",
  "@types/node": "^20.19.17",
  "@types/tar-stream": "^3.1.2",
  "@typescript-eslint/eslint-plugin": "^8.44.1",
  "@typescript-eslint/parser": "^8.44.0",
  "eslint": "^9.36.0",
  "eslint-config-prettier": "^10.1.8",
  "eslint-plugin-prettier": "^5.5.4",
  "prettier": "^3.6.2",
  "rimraf": "^5.0.0",
  "ts-node": "^10.9.1",
  "typescript": "^5.5.2",
  "typescript-eslint": "^8.44.0",
  "vitest": "^3.2.4"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "ignore": "^5.2.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "tar-stream": "^3.1.7"
